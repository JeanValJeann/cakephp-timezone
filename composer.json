{
    "name": "jeanvaljean/cakephp-timezone",
    "description": "Timezone and Datetime plugin for CakePHP 3.x",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "timezone",
        "datetime"
    ],
    "homepage": "https://github.com/JeanValJeann",
    "license": "MIT",
    "authors": [
        {
            "name": "PERRIN Jean-Charles",
            "homepage": "https://github.com/JeanValJeann",
            "role": "Author"
        }
    ],
    "support": {
        "source": "https://github.com/JeanValJeann/timezone-datetime"
    },
    "require": {
        "php": ">=5.6.0",
        "cakephp/cakephp": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0",
        "fig-r/psr2r-sniffer": "^0.5.0"
    },
    "autoload": {
        "psr-4": {
            "Timezone\\": "src/",
            "Timezone\\Test\\Fixture\\": "tests/Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Timezone\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
